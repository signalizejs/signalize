{
	"name": "signalizejs",
	"version": "0.1.0",
	"description": "A microframework inspired by SolidJS and Svelte, designed to easily create optimized and interactive websites with minimal JavaScript.",
	"keywords": [
		"signalize",
		"signalizejs",
		"performance",
		"components",
		"reactive",
		"signalize",
		"ui",
		"reactive",
		"components",
		"performance",
		"front-end",
		"framework",
		"web"
	],
	"type": "module",
	"sideEffects": false,
	"author": "Vladimír Macháček",
	"files": [
		"dist",
		"package.json",
		"LICENSE",
		"README.md"
	],
	"exports": {
		".": {
			"import": "./dist/signalize.js",
			"require": "./dist/signalize.cjs",
			"types": "./dist/index.d.ts"
		},
		"./dist/*": "./dist/*",

		"./ajax": {
			"import": "./ajax/dist/ajax.js",
			"require": "./ajax/dist/ajax.cjs",
			"types": "./ajax/dist/index.d.ts"
		},
		"./core": {
			"import": "./core/dist/core.js",
			"require": "./core/dist/core.cjs",
			"types": "./core/dist/index.d.ts"
		},
		"./dialog": {
			"import": "./dialog/dist/dialog.js",
			"require": "./dialog/dist/dialog.cjs",
			"types": "./dialog/dist/index.d.ts"
		},
		"./directives": {
			"import": "./directives/dist/directives.js",
			"require": "./directives/dist/directivs.cjs",
			"types": "./directives/dist/index.d.ts"
		},
		"./h": {
			"import": "./h/dist/h.js",
			"require": "./h/dist/h.cjs",
			"types": "./h/dist/index.d.ts"
		},
		"./asset-loader": {
			"import": "./asset-loader/dist/asset-loader.js",
			"require": "./asset-loader/dist/asset-loader.cjs",
			"types": "./asset-loader/dist/index.d.ts"
		},
		"./snippets": {
			"import": "./snippets/dist/snippets.js",
			"require": "./snippets/dist/snippets.cjs",
			"types": "./snippets/dist/index.d.ts"
		},
		"./spa": {
			"import": "./spa/dist/spa.js",
			"require": "./spa/dist/spa.cjs",
			"types": "./spa/dist/index.d.ts"
		},
		"./package.json": "./package.json"
	},
	"main": "dist/signalize.cjs",
	"jsdelivr": "dist/signalize.global.js",
	"unpkg": "dist/signalize.global.js",
	"module": "dist/signalize.js",
	"types": "dist/index.d.ts",
	"license": "MIT"
}
