{
	"name": "signalizejs",
	"version": "0.1.0",
	"description": "A microframework inspired by SolidJS and Svelte, designed to easily create optimized and interactive websites with minimal JavaScript.",
	"keywords": [
		"signalize",
		"signalizejs",
		"performance",
		"components",
		"reactive",
		"signalize",
		"ui",
		"reactive",
		"components",
		"performance",
		"front-end",
		"framework",
		"web"
	],
	"type": "module",
	"sideEffects": false,
	"author": "Vladimír Macháček",
	"files": [
		"dist",
		"package.json",
		"LICENSE",
		"README.md"
	],
	"exports": {
		".": {
			"import": "./dist/signalize.js",
			"require": "./dist/signalize.cjs",
			"types": "./dist/index.d.ts"
		},
		"./dist/*": "./dist/*",
		"./fetch": {
			"import": "./dist/plugins/fetch.js",
			"require": "./dist/plugins/fetch.cjs",
			"types": "./fetch/dist/fetch.d.ts"
		},
		"./dialog": {
			"import": "./dist/plugins/dialog.js",
			"require": "./dist/plugins/dialog.cjs",
			"types": "./dist/plugins/dialog.d.ts"
		},
		"./directives": {
			"import": "./dist/plugins/directives.js",
			"require": "./dist/plugins/directives.cjs",
			"types": "./dist/plugins/directives.d.ts"
		},
		"./h": {
			"import": "./dist/plugins/h.js",
			"require": "./dist/plugins/h.cjs",
			"types": "./dist/plugins/h.d.ts"
		},
		"./asset-loader": {
			"import": "./dist/plugins/asset-loader.js",
			"require": "./dist/plugins/asset-loader.cjs",
			"types": "./dist/plugins/asset-loader.d.ts"
		},
		"./snippets": {
			"import": "./dist/plugins/snippets.js",
			"require": "./dist/plugins/snippets.cjs",
			"types": "./dist/plugins/snippets.d.ts"
		},
		"./spa": {
			"import": "./dist/plugins/spa.js",
			"require": "./dist/plugins/spa.cjs",
			"types": "./dist/plugins/spa.d.ts"
		},
		"./package.json": "./package.json"
	},
	"main": "dist/signalize.cjs",
	"jsdelivr": "dist/signalize.global.js",
	"unpkg": "dist/signalize.global.js",
	"module": "dist/signalize.js",
	"types": "dist/index.d.ts",
	"license": "MIT"
}
